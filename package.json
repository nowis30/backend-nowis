{
  "name": "nowis-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
  "start:render": "node dist/scripts/startRender.js",
    "migrate:fallback": "prisma db push --accept-data-loss && npm run seed",
    "build": "tsc --project tsconfig.json",
    "render-build": "prisma generate && npm run build",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
  "test": "jest --runInBand",
  "smoke:gpt": "ts-node ./src/scripts/smoke-gpt.ts",
  "smoke": "ts-node ./src/scripts/smoke.ts",
  "validate:leverage": "ts-node ./src/scripts/validate-leverage.ts",
  "seed:monitoring:api": "ts-node ./src/scripts/seedMonitoringViaApi.ts",
    "seed:monitoring": "ts-node ./src/scripts/seedMonitoring.ts",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "seed": "ts-node ./src/scripts/seed.ts",
    "test:telemetry": "ts-node ./src/scripts/testTelemetry.ts"
  },
  "prisma": {
    "seed": "ts-node ./src/scripts/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@sparticuz/chromium": "^133.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pino": "^9.3.2",
    "pino-http": "^10.2.0",
  "puppeteer-core": "^23.4.0",
  "zod": "^3.23.8",
  "@napi-rs/canvas": "^0.1.51",
  "pdfjs-dist": "^4.7.76"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.24",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.30",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.10.0"
  },
  "engines": {
    "node": ">=18 <=22"
  }
}
